{
  "compilerOptions": {
    "target": "ES2023",
    "module": "ES2022",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "incremental": true,
    "declaration": true,
    "sourceMap": true,
    "composite": true,
    "moduleResolution": "bundler",
    "lib": ["DOM", "ESNext"]
  }
}
